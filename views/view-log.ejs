<% include partials/header%>

<% include partials/navigation%>

<hr>

<div class="container">
<div id="log-list">
        <% logs.forEach(function(log){ %>

            <div class="row log-item">
            
                <div class="col-md-3">
                    <div class="thumbnail">
                        <img src="<%= log.exercise.ImagePath %>" class="exercise-image" alt="<%= log.exercise.Name %>">
                    </div>
                </div>
                <div class="col-md-6">
                    <h3 class='exercise-name'> <%= log.exercise.Name %> </h3>
                </div>
                <div class="col-md-3">
                    <br>
                    <br>
                    <p>
                        Sets : <%=log.sets%>
                        Reps : <%=log.reps%>
                    </p>
                    <hr>
                </div>

            </div>
        <% }); %>

    </div>

<% include partials/footer%>